<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest Version="4.0" ExtensionBundleId="com.jakkor.AdobePProHttpsExample" ExtensionBundleVersion="1.0" ExtensionBundleName="Https with Adobe Premiere Pro Example">
    <ExtensionList>
        <Extension Id="com.jakkor.AdobePProHttpsExample.extension" Version="1.0" />
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="PPRO" Version="[8.0,9.9]" />
        </HostList>
        <LocaleList>
            <Locale Code="All" />
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="5.0" />
        </RequiredRuntimeList>
    </ExecutionEnvironment>

    <DispatchInfoList>
        <Extension Id="com.jakkor.AdobePProHttpsExample.extension">
            <DispatchInfo >
                <Resources>
                    <MainPath>./index.html</MainPath>
                    <CEFCommandLine>
                        <Parameter>--enable-nodejs</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>HttpsExample</Menu>
                    <Geometry>
                        <Size>
                            <Height>300</Height>
                            <Width>550</Width>
                        </Size>
                    </Geometry>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>


